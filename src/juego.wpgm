import wollok.game.*
import personaje.*
import detector.*
import configuracion.*
import spawner.*

program abc {

	configuracion.configuracionInicial()
	
	game.addVisual(personaje)
	
	detector.detectarTeclas()
	
	detector.detectarAvance()
	
	detector.detectarColiciones()
	
	spawner.spawn()
	
	game.start()
	
}